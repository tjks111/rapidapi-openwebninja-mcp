{
  "name": "openwebninja-mcp-server",
  "version": "1.0.0",
  "description": "Real-time web search MCP server using OpenWebNinja API via RapidAPI",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "openwebninja-mcp-server": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node --esm src/index.ts",
    "watch": "nodemon --exec ts-node --esm src/index.ts",
    "clean": "rimraf dist",
    "prepare": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "web-search",
    "openwebninja",
    "rapidapi",
    "google-search",
    "serp",
    "ai-tools"
  ],
  "author": "MCP Server Developer",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/openwebninja-mcp-server.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.4",
    "axios": "^1.11.0",
    "dotenv": "^16.4.7"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  }
}
